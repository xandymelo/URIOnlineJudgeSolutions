using System;
using System.Collections.Generic;
using System.Linq;
using static System.Boolean;

class MainClass {
  public static void Main (string[] args) {
    IEnumerable<int> sequencia = Enumerable.Range(0,100);
    foreach (int c in sequencia)
    {
      double elemento = Convert.ToDouble(Console.ReadLine());
      if (elemento <= 10)
      {
        Console.WriteLine($"A[{c}] = {elemento:f1}");
      }
    }
  }
  }